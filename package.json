    "babel-core": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "2.13.1",
    "eslint-config-permissible": "1.0.6",
    "expect": "1.20.2",
    "mocha": "2.5.3",
    "mocha-clean": "1.0.0",
